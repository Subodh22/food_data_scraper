axonomies": {"diet": [{"id": 29713, "name": "Vegetarian", "slug": "vegetarian"}], "difficulty": [{"id": 29638, "name": "Easy", "slug": "easy"}], "ingredient": [{"id": 35628, "name": "eggs", "slug": "eggs"}, {"id": 31673, "name": "feta", "slug": "feta"}, {"id": 32654, "name": "halloumi", "slug": "halloumi"}, {"id": 29723, "name": "olive oil", "slug": "olive-oil"}, {"id": 57396, "name": "pul biber", "slug": "pul-biber"}, {"id": 29815, "name": "red onion", "slug": "red-onion"}, {"id": 31205, "name": "tomatoes", "slug": "tomatoes"}, {"id": 34678, "name": "wild garlic", "slug": "wild-garlic"}], "meal-type": [{"id": 29890, "name": "Breakfast", "slug": "breakfast"}, {"id": 29892, "name": "Brunch", "slug": "brunch"}], "publication": [{"id": 62641, "name": "September 2021", "slug": "september-2021"}], "post_tag": [{"id": 31487, "name": "Eggs", "slug": "eggs"}, {"id": 34209, "name": "Feta", "slug": "feta"}, {"id": 35146, "name": "Halloumi", "slug": "halloumi"}, {"id": 31384, "name": "how to use up stale bread", "slug": "how-to-use-up-stale-bread"}, {"id": 34859, "name": "shakshuka", "slug": "shakshuka"}, {"id": 35250, "name": "Tomatoes", "slug": "tomatoes"}], "rating": [{"name": "1 Star+", "slug": "1-star"}, {"name": "2 Star+", "slug": "2-star"}, {"name": "3 Star+", "slug": "3-star"}, {"name": "4 Star+", "slug": "4-star"}], "calories": [{"name": "250 kcal and under", "slug": "250-kcal-and-under"}, {"name": "Up to 500 kcal", "slug": "up-to-500-kcal"}, {"name": "Up to 750 kcal", "slug": "up-to-750-kcal"}, {"name": "Up to 1000 kcal", "slug": "up-to-1000-kcal"}, {"name": "Up to 1500 kcal", "slug": "up-to-1500-kcal"}], "servings": [{"name": "6 servings", "slug": "6-servings"}], "total-time": [{"name": "1 hour or less", "slug": "1-hour-or-less"}]}, "act_time": 55, "act_yields": "6 serving(s)", "act_ingredients": ["6 medium tomatoes, halved", "2 tsp olive oil, plus a drizzle", "1 red onion, finely chopped", "8 padron peppers or 2 green sivri peppers (or use 1 green pepper if these aren\u2019t available), sliced into rounds", "2 tsp pul biber (aleppo chilli flakes), plus a pinch to serve", "handful of wild garlic or spinach", "100g stale bread of your choice, torn into bite-sized pieces", "3 eggs", "100g feta", "50g halloumi"], "act_instructions": "Heat the oven to 200C/180C fan/gas 6. Arrange the tomatoes cut-side down in a roasting tin, drizzle with some olive oil and season. Roast for 20 mins. Set aside to cool.\nMeanwhile, cook the onions in a medium ovenproof frying pan with the 2 tsp olive oil over a medium heat until they start to caramelise, about 8 mins.\nOnce the onions have softened, add the peppers and season with salt, pepper and the pul biber. Peel the skins from the roasted tomatoes, then add these to the pan of onions and peppers along with all the resting juices.\nCrush the tomatoes roughly with a wooden spoon and cook for 5 mins over a medium-high heat. Add the wild garlic or spinach and wilt for a minute, then remove from the heat. Tuck the bread pieces in amongst the mixture, then create three gaps for the eggs using the spoon. Crack the eggs into the gaps, crumble over the feta and grate over the halloumi. Drizzle with some extra olive oil and sprinkle with a pinch of pul biber.\nTurn the oven down to 160C/140C fan/gas 3 and bake the shakshuka for 12-15 mins. The eggs should be cooked but runny in the middle, and the feta, halloumi and bread should be crisp and golden.", "act_image": "https://images.immediate.co.uk/production/volatile/sites/30/2021/08/Feta-and-roasted-tomato-shakshuka-ce25544.jpg", "act_nutrients": {"calories": "186 calories", "fatContent": "10 grams fat", "saturatedFatContent": "5 grams saturated fat", "carbohydrateContent": "13 grams carbohydrates", "sugarContent": "5 grams sugar", "fiberContent": "3 grams fiber", "proteinContent": "11 grams protein", "sodiumContent": "1 milligram of sodium"}}